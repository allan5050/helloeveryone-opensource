{
  "name": "helloeveryone",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "jest --testNamePattern=\"Integration\"",
    "db:setup": "node scripts/setup-db.js",
    "db:seed": "node scripts/seed-db.js",
    "export-demo": "node scripts/export-demo-data.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "security:scan": "node scripts/security-scan.js",
    "security:scan:quick": "node scripts/security-scan.js --quick",
    "security:scan:ci": "node scripts/security-scan.js --ci",
    "security:secrets": "node scripts/check-secrets.js",
    "test:security": "jest --testPathPattern=tests/security --passWithNoTests",
    "test:security:edge": "jest --testPathPattern=tests/security/edge-cases --passWithNoTests",
    "test:security:business": "jest --testPathPattern=tests/security/business-logic --passWithNoTests",
    "test:security:all": "npm run test:security && npm run security:scan"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@hookform/resolvers": "^3.3.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "ics": "^2.44.0",
    "lucide-react": "^0.562.0",
    "next": "^15.5.9",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.3",
    "pngjs": "^7.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.1.2",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.23",
    "eslint": "^8",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "openai": "^4.20.0",
    "playwright": "^1.40.0",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
